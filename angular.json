{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shell": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/shell",
      "sourceRoot": "projects/shell/src",
      "prefix": "app-shell",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "styles": [
              "@angular/material/prebuilt-themes/azure-blue.css",
              "projects/shell/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "target": "shell:esbuild:production"
            },
            "development": {
              "target": "shell:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "shell:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/shell/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/shell/public"
              }
            ],
            "styles": [
              "projects/shell/src/styles.scss"
            ]
          }
        },
        "esbuild": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/shell/src/main.ts",
            "polyfills": [
              "zone.js",
              "es-module-shims"
            ],
            "tsConfig": "projects/shell/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/shell/public"
              }
            ],
            "styles": [
              "projects/shell/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-original": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "shell:esbuild:production"
            },
            "development": {
              "buildTarget": "shell:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200
          }
        }
      }
    },
    "products": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/products",
      "sourceRoot": "projects/products/src",
      "prefix": "app-products",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "styles": [
              "projects/products/src/custom-theme.scss"
            ]
          },
          "configurations": {
            "production": {
              "target": "products:esbuild:production"
            },
            "development": {
              "target": "products:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "products:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/products/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/products/public"
              }
            ],
            "styles": [
              "projects/products/src/styles.scss"
            ]
          }
        },
        "esbuild": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/products/src/main.ts",
            "polyfills": [
              "zone.js",
              "es-module-shims"
            ],
            "tsConfig": "projects/products/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/products/public"
              }
            ],
            "styles": [
              "projects/products/src/styles.scss"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-original": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "products:esbuild:production"
            },
            "development": {
              "buildTarget": "products:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201
          }
        }
      }
    }
  }
}
